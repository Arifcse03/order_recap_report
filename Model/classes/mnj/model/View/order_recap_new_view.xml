    Name="param"
    Kind="where"
    Type="oracle.jbo.domain.Number"/>
  <SQLQuery>
    <![CDATA[/* updated by wasi uddin*/ 
SELECT POC_ID,
       case   
           when LC_UNIT='340' THEN 'CAL Unit1' 
           when LC_UNIT='341' THEN 'CGL Unit1'
           when LC_UNIT='342' THEN 'GFL Unit1'
        end    
         LC_UNIT,
         ar.customer_name,
         BUYER_ID,
         SEASON,
         STYLE,
         WASH_NAME,
         COLOR,
         BPO,
         QTY,
         FOB,
         SHIPDATE,
         SHIPMODE,
         COUNTRY,
         SL_NO,
         bpo_id
    FROM (  SELECT POC_ID,
                   LC_UNIT,
                   BUYER_ID,
                   SEASON,
                   STYLE,
                   WASH_NAME,
                   COLOR,
                   BPO,
                   SUM (QTY) QTY,
                   FOB_WITH_SMS_SAMPLE FOB,
                   TO_CHAR (SHIPDATE, 'MM/DD/YYYY') SHIPDATE,
                   SHIPMODE,
                   COUNTRY,
                   1 SL_NO,
                   bpo_id
              FROM XX_ORDER_INFO_V v
             WHERE BPO_STATUS = 1 AND qty > 0   and 1 = :param     --AND SHIPMODE IS NOT NULL
          GROUP BY LC_UNIT,
                   POC_ID,
                   FOB_ID,
                   SYSTEM_ID,
                   BUYER_ID,
                   SEASON,
                   STYLE,
                   WASH_NAME,
                   COLOR,
                   BPO_ID,
                   BPO,
                   FOB_WITH_SMS_SAMPLE,
                   SHIPDATE,
                   SHIPMODE,
                   COUNTRY,
                   bpo_id
          UNION ALL
            SELECT POC_ID,
                   LC_UNIT,
                   BUYER_ID,
                   SEASON,
                   STYLE,
                   WASH_NAME,
                   COLOR,
                   '' BPO,
                   SUM (QTY) QTY,
                   AVG (FOB_WITH_SMS_SAMPLE),
                   '' SHIPMODE,
                   '' abc,                           -- SUM (QTY) * AVG (FOB),
                   '' COUNTRY,
                   2 SL_NO,
                   0 bpo_id
              FROM XX_ORDER_INFO_V v
             WHERE BPO_STATUS = 1    and 1 = :param                --AND SHIPMODE IS NOT NULL
          GROUP BY LC_UNIT,
                   POC_ID,
                   FOB_ID,
                   SYSTEM_ID,
                   BUYER_ID,
                   SEASON,
                   STYLE,
                   WASH_NAME,
                   COLOR
          UNION ALL
            SELECT 0 POC_ID,
                   LC_UNIT,
                   BUYER_ID,
                   SEASON,
                   '' STYLE,
                   '' WASH_NAME,
                   '' COLOR,
                   '' BPO,
                   SUM (QTY) QTY,
                   AVG (FOB),
                   '' SHIPDATE,
                   '' SHIPMODE,
                   '' COUNTRY,
                   3 SL_NO,
                   0 BPO_ID
              FROM XX_ORDER_INFO_V v
             WHERE BPO_STATUS = 1 and 1 = :param 
          GROUP BY LC_UNIT, BUYER_ID, SEASON) M, Ar_customers ar where ar.CUSTOMER_ID=M.buyer_id  
   
ORDER BY style,
         color,
         wash_name,
         FOB,
         SL_NO ASC]]>
  </SQLQuery>
