  <SQLQuery>
    <![CDATA[SELECT BUYER_ID ,
(select customer_name
          from ar_customers ac
         where ac.CUSTOMER_ID =  MNJ_ORDER_RECAP_RECONCILE .Buyer_Id) buyer,
         SEASON,
        style_sl,
       style,
       fit||' '||item_description description,
       wash_name,
       colour,
        po_type,
        bpo_number,
        nvl(replace_bpo,bpo_number) PO_NO,
       classification,
       replace_bpo,
       ref_tba,
       ordered_quantity,
       unit_selling_price,
       total_value,
       
       ship_ex_fac_date,
       ship_cancel_bd_date,
       item_desc,
       item_description_poc,
       inco_terms,
       style_name,
        fit,
        end_user,
        ORG_ID,
        item_description
       
  FROM MNJ_ORDER_RECAP_RECONCILE 
-- WHERE SEASON = :P_SEASON
-- AND BUYER_ID = :P_BUYER_ID
--  AND ORG_ID = :P_ORG_ID
order by style,style_sl]]>
  </SQLQuery>
