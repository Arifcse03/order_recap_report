<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="BuyerLOV"
  Version="11.1.1.62.29"
  InheritPersonalization="true"
  BindingStyle="OracleName"
  CustomQuery="true"
  PageIterMode="Full"
  UseGlueCode="false">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="mnj.model.View.BuyerLOV_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <SQLQuery>
    <![CDATA[select DISTINCT ac.CUSTOMER_ID,
       ac.CUSTOMER_NUMBER,
       ac.CUSTOMER_NAME 
from ar_customers ac,
     qa_results_v qr 
WHERE qr.NAME = 'BUYER WISE DETAILS OM'
AND ac.CUSTOMER_ID = qr.CHARACTER15 and ac.status = 'A'  and CUSTOMER_NAME_PHONETIC='M']]>
  </SQLQuery>
  <ViewAttribute
    Name="CustomerId"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="15"
    Scale="0"
    Type="oracle.jbo.domain.Number"
    ColumnType="NUMBER"
    AliasName="CUSTOMER_ID"
    Expression="CUSTOMER_ID"
    SQLType="NUMERIC">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="22"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
        <LABEL
          ResId="mnj.model.View.BuyerLOV.CustomerId_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerNumber"
    IsUpdateable="false"
    IsPersistent="false"
    IsNotNull="true"
    PrecisionRule="true"
    Precision="30"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUSTOMER_NUMBER"
    Expression="CUSTOMER_NUMBER"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="30"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYHINT
          Value="Hide"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <ViewAttribute
    Name="CustomerName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="50"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="CUSTOMER_NAME"
    Expression="CUSTOMER_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="50"/>
    </DesignTime>
    <Properties>
      <SchemaBasedProperties>
        <DISPLAYWIDTH
          Value="20"/>
        <LABEL
          ResId="mnj.model.View.BuyerLOV.CustomerName_LABEL"/>
      </SchemaBasedProperties>
    </Properties>
  </ViewAttribute>
  <LOVUIHints
    ListType="105"
    MRUCount="0"
    ListRangeSize="-1"
    NullValueId="LOVUIHints_NullValueId"
    NullValueFlag="none">
    <AttrArray Name="ListDisplayAttrNames">
      <Item Value="CustomerId"/>
      <Item Value="CustomerName"/>
    </AttrArray>
    <DisplayCriteria/>
  </LOVUIHints>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="mnj.model.ModelBundle"/>
  </ResourceBundle>
</ViewObject>
